    ,
    {
      "type" : "verse",
      "id" : "v14",
      "number" : "14",
      "absolute" : "15863",
      "text" : "I will pay my vows unto the <span class='nog'>Lord<\/span> now in the presence of all his people."
    }
    ,
    {
      "type" : "verse",
      "id" : "v15",
      "number" : "15",
      "absolute" : "15864",
      "text" : "Precious in the sight of the <span class='nog'>Lord<\/span> <span class='extra'>is<\/span> the death of his saints."
    }
    ,
    {
